{
  "name": "glamor-aphrodite",
  "version": "3.0.0-1",
  "description": "inline css for component systems",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "files": [
    "es6",
    "lib",
    "umd",
    "index.d.ts"
  ],
  "scripts": {
    "example": "lerna exec --scope glamor-aphrodite-example npm run start",
    "clean": "rimraf lib es6 umd",
    "build": "npm-run-all -s clean -p build:** -s size",
    "build:lib": "babel src -d lib --presets es2015,stage-0,react",
    "build:es6": "babel src -d es6",
    "build:umd": "mkdirp umd && webpack --config ../../webpack/umd.js",
    "build:umd-prod": "cross-env NODE_ENV=production npm run build:umd",
    "size": "cat umd/index.min.js | gzip | wc -c",
    "test": "karma start ../../karma.conf.js",
    "test:watch": "npm test -- --no-single-run",
    "coverage": "rm -rf tests/coverage && COVERAGE=1 npm test && open tests/coverage/*/index.html",
    "prepublish": "npm run build"
  },
  "author": "Sunil Pai <threepointone@gmail.com>",
  "homepage": "https://github.com/threepointone/glamor",
  "license": "MIT",
  "repository": "https://github.com/threepointone/glamor",
  "keywords": [
    "css",
    "inline"
  ],
  "dependencies": {
    "glamor": "^3.0.0-1"
  },
  "devDependencies": {
    "@types/expect": "^1.20.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "caniuse-api": "^2.0.0",
    "cross-env": "^4.0.0",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^1.8.1",
    "lerna": "2.0.0-rc.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "2.4.5",
    "pegjs": "^0.10.0",
    "pug": "^2.0.0-beta6"
  }
}
