{
  "name": "glamor-ous",
  "version": "3.0.0-1",
  "description": "inline css for component systems",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "author": "Sunil Pai <threepointone@gmail.com>",
  "homepage": "https://github.com/threepointone/glamor",
  "license": "MIT",
  "repository": "https://github.com/threepointone/glamor",
  "scripts": {
    "example": "../../node_modules/.bin/lerna exec --scope glamor-ous-example npm run start",
    "clean": "../../node_modules/.bin/rimraf lib es6 umd",
    "build": "../../node_modules/.bin/npm-run-all -s clean -p build:** -s size",
    "build:lib": "../../node_modules/.bin/babel src -d lib --presets es2015,stage-0,react",
    "build:es6": "../../node_modules/.bin/babel src -d es6",
    "build:umd": "../../node_modules/.bin/mkdirp umd && ../../node_modules/.bin/webpack --config ../../webpack/umd.js",
    "build:umd-prod": "../../node_modules/.bin/cross-env NODE_ENV=production npm run build:umd",
    "size": "cat umd/index.min.js | gzip | wc -c",
    "test": "echo \"Write more tests :)\" && exit 0",
    "prepublish": "npm run build"
  },
  "keywords": [
    "css",
    "inline"
  ],
  "dependencies": {
    "glamor": "^3.0.0-1"
  }
}
